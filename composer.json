{
    "name": "delighted/delighted",
    "type": "library",
    "description": "Delighted PHP API Client",
    "keywords": ["delighted"],
    "license": "MIT",
    "homepage": "http://github.com/delighted/delighted-php",
    "authors": [
        {
            "name": "Ben Turner",
            "email": "ben@delighted.com"
        },
        {
            "name": "David Sklar",
            "email": "david.sklar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "php-http/client-implementation": "^1.0",
        "php-http/httplug": "^1.0",
        "php-http/message-factory": "^1.0",
        "php-http/discovery": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "php-http/mock-client": "^1.0",
        "php-http/guzzle6-adapter": "^1.0",
        "symfony/var-dumper": "^3.2"
    },
    "autoload": {
        "psr-0": {
            "Delighted": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Delighted\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "php-http/curl-client": "cURL client for PHP-HTTP",
        "guzzlehttp/psr7": "PSR-7 message implementation that also provides common utility methods"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-2.x": "2.0-dev"
        }
    }
}
